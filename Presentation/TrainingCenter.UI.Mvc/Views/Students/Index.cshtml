@model IEnumerable<StudentViewModel>

@{
    ViewData["Title"] = "All Students";
}

<h1>All Students</h1>

<table class="table table-stripped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Full Name</th>
            <th>Email</th>
            <th>Mobile</th>
        </tr>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.FullName</td>
                <td>@student.Email</td>
                <td>@student.Mobile</td>
            </tr>
        }
    </thead>
</table>